@page
@using SaludIntegral.App.Dominio
@model SaludIntegral.App.Frontend.Pages.showMedicsModel
@{
}
<h1>Listado de Medicos</h1>
<table class="table table-striped">
    @foreach(var medico in Model.medicos)
    {
        <tr>
            <td>@medico.Names</td>
            <td>@medico.Identification</td>
            <td>@medico.Specialization</td>
        </tr>
    }
</table>

