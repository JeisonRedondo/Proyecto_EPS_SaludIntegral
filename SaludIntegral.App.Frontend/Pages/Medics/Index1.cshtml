@page
@model SaludIntegral.App.Frontend.Pages.Medics.Index1Model
@{
}
<h1>Listado de Medicos</h1>
<table class="table table-striped">
    <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Email</th>
        <th scope="col">Especialidad</th>
    </tr>
    @foreach (var medico in Model.medicos)
    {
        <tr>
            <td>@medico.Names</td>
            <td>@medico.Email</td>
            <td>@medico.Specialization</td>
        </tr>
    }
</table>
